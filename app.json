{
  "expo": {
    "name": "Social media app",
    "slug": "diplomaproject",
    "version": "1.2.0",
    "orientation": "default",
    "icon": "./assets/icon.png",
    "userInterfaceStyle": "light",
    "runtimeVersion": "1.2.0",
    "plugins": [
      [
        "expo-notifications",
        {
          "icon": "./assets/icon.png",
          "color": "#ffffff",
          "sounds": [
            "./local/assets/notification-sound.wav",
            "./local/assets/notification-sound-other.wav"
          ]
        }
      ]
    ],
    "splash": {
      "image": "./assets/splash.png",
      "resizeMode": "contain",
      "backgroundColor": "#ffffff"
    },
    "assetBundlePatterns": [
      "**/*"
    ],
    "ios": {
      "googleServicesFile": "./GoogleService-Info.plist",
      "buildNumber": "1",
      "infoPlist": {
        "NSPhotoLibraryUsageDescription": "Доступ к вашей фотографии",
        "NSCameraUsageDescription": "Доступ к вашей камере",
        "UIBackgroundModes": [
          "location",
          "fetch"
        ]
      },
      "bundleIdentifier": "com.meruem.diplomka"
    },
    "android": {
      "googleServicesFile": "./google-services.json",
      "useNextNotificationsApi": true,
      "permissions": [],
      "adaptiveIcon": {
        "foregroundImage": "./assets/adaptive-icon.png",
        "backgroundColor": "#ffffff"
      },
      "versionCode": 1,
      "package": "com.meruem.diplomka"
    },
    "web": {
      "favicon": "./assets/favicon.png"
    },
    "owner": "meruem",
    "extra": {
      "eas": {
        "projectId": "4ea3e22e-3ad3-4c4e-95c0-937eede4f235"
      }
    },
    "updates": {
      "url": "https://u.expo.dev/4ea3e22e-3ad3-4c4e-95c0-937eede4f235"
    }
  }
}
